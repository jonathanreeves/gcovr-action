{
  "plugins": ["jest", "@typescript-eslint"],
  "extends": ["plugin:github/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "./tsconfig.eslint.json"
  },
  "rules": {
    "camelcase": "off",
    "i18n-text/no-en": "off",
    "import/no-namespace": "off"
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}
